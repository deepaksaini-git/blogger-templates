$(function($) { $["fn"].lazyimg = function() { return this.each(function() { function delayStateChange() { var siteName = $(window).height(); var currentw = $(window).scrollTop(); var maxwidth = src_item["offset"]().top; if (currentw + siteName > maxwidth) { var img = new Image; img.onload = function() { src_item.addClass("lazy-img"); }; img.src = normalSrc; } } var src_item = $(this); var part01 = src_item.attr("src"); var height = Math["round"](src_item.height()); var normalSrc = ""; if (part01["match"]("s72-c")) { normalSrc = part01.replace("/s72-c", height); } else { normalSrc = part01; } $(window)["on"]("load scroll", delayStateChange); delayStateChange(); }); }; });

$(document).ready(function() { $('.shareMore .button, .closeCheck, .shareShow, #overlayOk').on('click', function() { $('body').toggleClass('shareShow'); }); });

$("#socialInside").appendTo(".socialInside");

$(document).ready(function() {
    let lastScrollTop = 0;
    $(window).scroll(function() {
        let currentScrollTop = $(this).scrollTop();
        if (currentScrollTop > lastScrollTop) {
            $('header').removeClass('scrolled-up').addClass('scrolled-down');
        } else {
            $('header').removeClass('scrolled-down').addClass('scrolled-up');
        }
        lastScrollTop = currentScrollTop;
    });
});


$('.socialFooter a, .shareMore a').on('click', function(e) {
    e.preventDefault();
    const url = $(this).data('url');
    window.open(url, '_blank');
});


